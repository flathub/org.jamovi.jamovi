{
    "name": "medmod",
    "buildsystem": "simple",
    "build-commands": [
        "mkdir -p /app/lib/jamovi/modules-extra",
        "mkdir -p module/build/R",
        "R CMD INSTALL *.Rpkg --library=module/build/R --no-docs --no-html --no-data --no-help --no-demo --no-multiarch",
        "jmc --install module --to /app/lib/jamovi/modules-extra --home /app"
    ],
    "sources": [
        {
            "type": "git",
            "url": "https://github.com/raviselker/medmod.git",
            "commit": "6e9ec3bb96f58f9120beb23b495903c661359da5",
            "dest": "module"
        },
        "medmod-deps.json"
    ]
}
